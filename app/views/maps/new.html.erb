<h2>[New html] Create new map</h2>

<%=form_for @map, :url=> maps_path, do |f|%>

  <%#= render :partial => 'form', :locals => { :f => f } %>


    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <%= f.label :country, "Country" %>
    <%= f.text_field :country %>
    <%= f.label :time, "Time" %>
    <%= f.text_field :time %>
    <%= f.label :money, "Money" %>
    <%= f.text_field :Money %>
    <%= f.label :days, "days" %>
    <%= f.text_field :days %>
    <%= f.label :description, "Description" %>
    <%= f.text_area :description %>

<%f.submit 'create' %>

<% end %>